{
    "name": "xmsjdev/ip2region",
	"version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "ip2region"
    ],
    "description": "hyperf ip2region",
    "autoload": {
        "psr-4": {
            "Xmsjdev\\Ip2region\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "hyperf/testing": "~3.1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": ">=7.0",
        "swoole/ide-helper": "^4.5",
        "swow/swow": "dev-develop",
        "symfony/var-dumper": "^5.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Xmsjdev\\Ip2region\\ConfigProvider"
        }
    }
}
